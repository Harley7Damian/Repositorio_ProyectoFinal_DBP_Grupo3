@model IEnumerable<ProyectoDesarrollo.B.P.Models.Usuario>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
<head>
</head>
<body>
    <table border="2">
        <tr>
            <td>CODIGO</td>
            <td>NOMBRE</td>
            <td>APELLIDO</td>
            <td>EDAD</td>
            <td>CARGAR</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Apellido</td>
                <td>@item.Edad</td>
                <td><a asp-action="Cargar" asp-route-id="@item-Id">Cargar</a></td>
            </tr>
        }
    </table>
</body>
</html>
